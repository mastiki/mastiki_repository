							"type": "PipelineReference"
						},
						"parameters": {
							"ParameterNumberOfDays": "[parameters('TriggerBillingPerEndpoint_properties_BillingPerEndpointPipeline_parameters_ParameterNumberOfDays')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Day",
						"interval": 1,
						"startTime": "2020-08-24T22:00:00Z",
						"timeZone": "UTC"
					}
				}
